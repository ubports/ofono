[Unit]
Description=Telephony service
Requires=dbus.service
After=dbus.service

[Service]
Type=dbus
BusName=org.ofono
User=root
EnvironmentFile=-/var/lib/environment/ofono/*.conf
ExecStart=@prefix@/sbin/ofonod -n --nobacktrace $OFONO_ARGS $OFONO_DEBUG
StandardError=null
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
